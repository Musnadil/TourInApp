<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/splashScreenFragment">

    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="com.indexdev.tourin.ui.splashscreen.SplashScreenFragment"
        android:label="fragment_splash_screen"
        tools:layout="@layout/fragment_splash_screen" >
        <action
            android:id="@+id/action_splashScreenFragment_to_firstOnBoardingFragment"
            app:destination="@id/firstOnBoardingFragment"
            app:popUpTo="@id/splashScreenFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_splashScreenFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/splashScreenFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_splashScreenFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/splashScreenFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_splashScreenFragment_to_ratingFragment"
            app:destination="@id/ratingFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/splashScreenFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/firstOnBoardingFragment"
        android:name="com.indexdev.tourin.ui.onboarding.FirstOnBoardingFragment"
        android:label="fragment_first_on_boarding"
        tools:layout="@layout/fragment_first_on_boarding">
        <action
            android:id="@+id/action_firstOnBoardingFragment_to_secondOnBoardingFragment"
            app:destination="@id/secondOnBoardingFragment"
            app:popUpTo="@id/firstOnBoardingFragment"
            app:popUpToInclusive="true"
            app:exitAnim="@anim/slide_out"
            app:enterAnim="@anim/slide_in"/>
    </fragment>
    <fragment
        android:id="@+id/secondOnBoardingFragment"
        android:name="com.indexdev.tourin.ui.onboarding.SecondOnBoardingFragment"
        android:label="fragment_second_on_boarding"
        tools:layout="@layout/fragment_second_on_boarding" >
        <action
            android:id="@+id/action_secondOnBoardingFragment_to_thirdOnBoardingFragment"
            app:destination="@id/thirdOnBoardingFragment"
            app:popUpTo="@id/secondOnBoardingFragment"
            app:popUpToInclusive="true"
            app:exitAnim="@anim/slide_out"
            app:enterAnim="@anim/slide_in"/>
    </fragment>
    <fragment
        android:id="@+id/thirdOnBoardingFragment"
        android:name="com.indexdev.tourin.ui.onboarding.ThirdOnBoardingFragment"
        android:label="fragment_third_on_boarding"
        tools:layout="@layout/fragment_third_on_boarding" >

        <action
            android:id="@+id/action_thirdOnBoardingFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpToInclusive="true"
            app:popUpTo="@id/thirdOnBoardingFragment"
            app:exitAnim="@anim/slide_out"
            app:enterAnim="@anim/slide_in"/>
        <action
            android:id="@+id/action_thirdOnBoardingFragment_to_homeFragment2"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/slide_out"
            app:popUpTo="@id/thirdOnBoardingFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.indexdev.tourin.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_mapsFragment"
            app:destination="@id/mapsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_homeFragment_to_descFragment"
            app:destination="@id/descFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.indexdev.tourin.ui.auth.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/slide_out"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/zoom_in"/>
        <action
            android:id="@+id/action_loginFragment_to_forgotPasswordFragment"
            app:destination="@id/forgotPasswordFragment"
            app:popUpTo="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.indexdev.tourin.ui.auth.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/right_slide_in"
            app:exitAnim="@anim/right_slide_out"
            app:popUpTo="@id/registerFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.indexdev.tourin.ui.maps.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" >
        <action
            android:id="@+id/action_mapsFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_mapsFragment_to_productPartnerFragment"
            app:destination="@id/productPartnerFragment" />
    </fragment>
    <fragment
        android:id="@+id/ratingFragment"
        android:name="com.indexdev.tourin.ui.rating.RateFragment"
        android:label="fragment_rating"
        tools:layout="@layout/fragment_rating" >
        <action
            android:id="@+id/action_ratingFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/ratingFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/productPartnerFragment"
        android:name="com.indexdev.tourin.ui.productpartner.ProductPartnerFragment"
        android:label="fragment_product_partner"
        tools:layout="@layout/fragment_product_partner" >
        <action
            android:id="@+id/action_productPartnerFragment_to_profilePartnerFragment"
            app:destination="@id/profilePartnerFragment"
            app:exitAnim="@anim/slide_out"
            app:enterAnim="@anim/slide_in"/>
        <action
            android:id="@+id/action_productPartnerFragment_to_detailProductFragment"
            app:destination="@id/detailProductFragment"
            app:exitAnim="@anim/slide_out"
            app:enterAnim="@anim/slide_in"/>
    </fragment>
    <fragment
        android:id="@+id/profilePartnerFragment"
        android:name="com.indexdev.tourin.ui.profilepartner.ProfilePartnerFragment"
        android:label="fragment_profile_partner"
        tools:layout="@layout/fragment_profile_partner" />
    <fragment
        android:id="@+id/detailProductFragment"
        android:name="com.indexdev.tourin.ui.detailproduct.DetailProductFragment"
        android:label="fragment_detail_produk"
        tools:layout="@layout/fragment_detail_produk" />
    <fragment
        android:id="@+id/forgotPasswordFragment"
        android:name="com.indexdev.tourin.ui.forgotpassword.ForgotPasswordFragment"
        android:label="fragment_forgot_password"
        tools:layout="@layout/fragment_forgot_password" >
        <action
            android:id="@+id/action_forgotPasswordFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/forgotPasswordFragment" />
        <action
            android:id="@+id/action_forgotPasswordFragment_to_verifyOtpFragment"
            app:destination="@id/verifyOtpFragment"
            app:popUpTo="@id/forgotPasswordFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/verifyOtpFragment"
        android:name="com.indexdev.tourin.ui.verifyotp.VerifyOtpFragment"
        android:label="fragment_verify_otp"
        tools:layout="@layout/fragment_verify_otp" >
        <action
            android:id="@+id/action_verifyOtpFragment_to_forgotPasswordFragment"
            app:destination="@id/forgotPasswordFragment"
            app:popUpTo="@id/verifyOtpFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_verifyOtpFragment_to_newPasswordFragment"
            app:destination="@id/newPasswordFragment"
            app:popUpTo="@id/verifyOtpFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/newPasswordFragment"
        android:name="com.indexdev.tourin.ui.newpassword.NewPasswordFragment"
        android:label="fragment_new_password"
        tools:layout="@layout/fragment_new_password" >
        <action
            android:id="@+id/action_newPasswordFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/newPasswordFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/listRateFacilityFragment"
        android:name="com.indexdev.tourin.ui.listratefacility.ListRateFacilityFragment"
        android:label="fragment_list_rate_facility"
        tools:layout="@layout/fragment_list_rate_facility" />
    <fragment
        android:id="@+id/descFragment"
        android:name="com.indexdev.tourin.ui.desc.DescFragment"
        android:label="fragment_desc"
        tools:layout="@layout/fragment_desc" >
        <action
            android:id="@+id/action_descFragment_to_mapsFragment"
            app:destination="@id/mapsFragment" />
        <action
            android:id="@+id/action_descFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>

</navigation>